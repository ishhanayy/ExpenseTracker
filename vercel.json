{
    "builds": [
      {
        "src": "expenseTracker/wsgi.py",
        "use": "@vercel/python",
        "config": {
          "maxLambdaSize": "15mb",
          "runtime": "python3.9",
          "buildCommand": "apt-get update && apt-get install -y libmysqlclient-dev && pip install -r requirements.txt"
        }
      }
    ],
    "env": {
      "MYSQL_ROOT_PASSWORD": "root",
      "MYSQL_DATABASE": ""
    },
    "routes": [
      {
        "src": "/(.*)",
        "dest": "expenseTracker/wsgi.py"
      }
    ]
  }
  